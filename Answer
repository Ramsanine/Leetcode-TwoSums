


class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        for (int i = 0; i  < nums.length - 1; i++){
            for (int j = i+1; j < nums.length; j++){
                if (nums[i] + nums[j] == target){
                    vector<int> result = [i, j]
                    return result;
                }
            }
        }
        
    }
};
